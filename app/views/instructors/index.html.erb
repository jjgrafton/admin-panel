<h1 class="admin-form">Instructors</h1>

<% @instructors.each do |instructor| %>
<div class="well">
    <h2 class="body-text"><%= instructor.first_name %></h2>
        <h2><%= instructor.last_name %></h2>
    <%= link_to "Full Profile", instructor_path(instructor), class: 'btn btn-primary', class: 'admin-form' %>
</div>


<% end %>