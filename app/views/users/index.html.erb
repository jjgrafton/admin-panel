<div class="login">
  <div class="container">
    <div class="form">

    <h1>Login</h1>
<% form_for @user do |f| %>
    <%= f.error_messages %>
    <p>
        <%= f.label :username %>
        <%= f.text_field :password %>
    </p>
    <p>
        <%= f.label :password %>
        <%= f.password_field :password %>
    </p>

    <%= f.submit 'Login' %>
<% end %>

  
    </div>
  </div>
</div>