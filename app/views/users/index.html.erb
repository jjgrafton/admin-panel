<%= render 'shared/signin' %>

<div class="login">
  <div class="container">
    <div class="form">

    <h1 class="admin-form">Login</h1>
<% form_for @admin do |f| %>
    <%= f.error_messages %>
    <p class="body-text">
        <%= f.label :username %>
        <%= f.text_field :password %>
    </p>
    <p class="body-text">
        <%= f.label :password %>
        <%= f.password_field :password %>
    </p>

    <%= f.submit 'Login', class: 'admin-form' %>
<% end %>

  
    </div>
  </div>
</div>